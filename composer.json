{
    "name": "juling/think-foundation",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.4",
        "topthink/framework": "^8.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Juling\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
